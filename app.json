{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-camera",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to upload invoice images.",
          "cameraPermission": "The app needs camera access to capture invoice photos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
